<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<!-- Reglas expedientes y programacion -->
	<navigation-rule>
		<from-view-id>/kernel/expedientes/*</from-view-id>
		<navigation-case>
			<from-outcome>actexpediente</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/listado.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>proginformes</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/listado.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Reglas administracion de expedientes de expedientes y programacion -->
	<navigation-rule>
		<from-view-id>/kernel/expedientes/actexpediente/*</from-view-id>
		<navigation-case>
			<from-outcome>agregar</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/agregar.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizar</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/actualizar.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminar</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/eliminar.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>transaccionExitosa</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/listado.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>detalle</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/verExpediente.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cambiarEtapaExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/cambiaetapaexp.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>infoExpedienteExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/infoexpediente.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>duplicarExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/duplexexpediente.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>devolucionExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/devolucionRechazo.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>adjArchivosExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/adjuntaArchivos.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>genDocumentosExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/generaDocumento.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregarAnotacionExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/anotacionesexp/agregarAno.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizarAnotacionExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/anotacionesexp/actualizarAno.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminarAnotacionExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/anotacionesexp/eliminarAno.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregarAnexoExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/anexosexp/agregarAnx.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizarAnexoExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/anexosexp/actualizarAnx.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminarAnexoExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/anexosexp/eliminarAnx.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregarOtroExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/otrosexp/agregarOtro.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizarOtroExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/otrosexp/actualizarOtro.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminarOtroExp</from-outcome>
			<to-view-id>/kernel/expedientes/actexpediente/otrosexp/eliminarOtro.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listadoRegistros</from-outcome>
			<to-view-id>/kernel/expedienteRegistro/registro/listado.jspx</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>


	<!-- Reglas Control de informes de expedientes y programacion -->
	<navigation-rule>
		<from-view-id>/kernel/expedientes/proginformes/*</from-view-id>
		<navigation-case>
			<from-outcome>agregar</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/agregar.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizar</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/actualizar.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminar</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/eliminar.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>transaccionExitosa</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/listado.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>detalleProg</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/detalleprogramacion.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agregarDetalleProg</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/detalleprog/agregarDetalleProg.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>actualizarDetalleProg</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/detalleprog/actualizarDetalleProg.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>eliminarDetalleProg</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/detalleprog/eliminarDetalleProg.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>duplicarDetalleProg</from-outcome>
			<to-view-id>/kernel/expedientes/proginformes/detalleprog/duplicarDetalleProg.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
