 ul.tabsCustom {
	margin: 0;
	padding: 0;
	float: left;
	list-style: none;
	height: 32px;
	/*border-bottom: 1px solid #999;
	border-left: 1px solid #999;*/
	font-weight: bold;
	width: 100%;
}
ul.tabsCustom li {
	float: left;
	margin: 0;
	padding: 0;
	height: 31px;
	line-height: 31px;
	border: 1px solid #9dffe9;
	/*border-left: none;*/
	margin-bottom: -1px;
	overflow: hidden;
	position: relative;
	background: #e0e0e0;
	-webkit-border-top-left-radius: 8px;
	-webkit-border-top-right-radius: 8px;
	-moz-border-radius-topleft: 8px;
	-moz-border-radius-topright: 8px;
	border-top-left-radius: 8px;
	border-top-right-radius: 8px;
}
ul.tabsCustom li table {
	padding: 0 5px;
}
ul.tabsCustom li a {
	text-decoration: none;
	color: #000;
	display: block;
    font-family: Arial,Verdana,sans-serif;
    font-size: 12px;
	/*padding: 0 15px;
	/*border: 1px solid #fff;*/
	outline: none;
}
ul.tabsCustom li a:hover {
	background: #ccc;
}

html ul.tabsCustom li.active, html ul.tabsCustom li.active a:hover  {
	background: #fff;
	border-bottom: 1px solid #fff;
}

